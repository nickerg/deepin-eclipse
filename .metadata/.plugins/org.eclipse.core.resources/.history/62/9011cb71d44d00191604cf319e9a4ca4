package com.walk.multi;

public class MultiTable00 {
/**
 * 实现打印九九乘法表
 * @param args
 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		print1();
	}
	public static void print1() {
		for (int i = 1; i <= 9; i++) {
			for (int j = 1; j < i+1; j++) {
				System.out.print(j+"*"+i+"="+i*j+"\t");
			}
			System.out.println();
		}
	}

}
