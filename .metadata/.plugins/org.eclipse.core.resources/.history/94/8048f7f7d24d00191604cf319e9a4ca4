package com.walk.multi;

public class MultiTable00 {
/**
 * 实现打印九九乘法表
 * @param args
 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		print1();
	}
	public static void print1() {
		for (int i = 0; i < 9; i++) {
			for (int j = 0; j < i+1; j++) {
				System.out.println(i+"*"+j+"="+i*j+"\t");
			}
		}
	}

}
